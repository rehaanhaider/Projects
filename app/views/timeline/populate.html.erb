<div class = "container">
  <h3> TimeLine </h3>
  <table class="table table-striped">
    <tr>
      <th>Title</th>
      <th>Text</th>
      <th>Username</th>
    </tr>
  
    <% @timeline_articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.text %></td>
        <td><%= User.find(article.user_id).name %></td>
      </tr>
    <% end %>
  </table>

  <br/>
  <br/>
  <%= link_to 'Back', user_path(current_user.id), class: 'btn btn-primary' %>
  

</div>